<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chat_application.IndexController">
<AnchorPane prefHeight="556.0" prefWidth="8401.0">
   <children>
         <ImageView fitHeight="185.0" fitWidth="326.0" layoutX="187.0" layoutY="109.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@images.png" />
            </image>
         </ImageView>
         <ImageView fitHeight="225.0" fitWidth="318.0" layoutX="191.0" layoutY="379.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@image2.jpeg" />
            </image>
         </ImageView>
      <Button layoutX="310.0" layoutY="517.0" mnemonicParsing="false" onAction="#onCreateClick" style="-fx-background-color: black;" text="Create room" textFill="WHITE" />
      <Button layoutX="316.0" layoutY="250.0" mnemonicParsing="false" onAction="#onJoinClick" style="-fx-background-color: black;" text="Join room" textFill="WHITE" />
         <Button layoutX="549.0" layoutY="2.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="81.0" style="-fx-background-color: BLACK;" text="LOGOUT" textFill="#fffefe" onAction="#onLogout">
            <font>
               <Font size="15.0" />
            </font>
         </Button>
   </children>
</AnchorPane>
</VBox>
